{
    "manifest_version": 2,
    "name": "National Portrait Gallery Scavenger Hunt",
    "description": "",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "npg-icon.png"
    },
    "web_accessible_resources": [
        "icons/witness.svg"
    ]
}